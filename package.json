{
  "name": "pantheon-god-agent-system",
  "version": "1.0.0",
  "description": "A divine AI agent orchestration system extending Claude-Flow with 16 specialized god agents",
  "type": "module",
  "main": "launch.js",
  "scripts": {
    "start": "node launch.js",
    "dev": "node launch.js --verbose",
    "test": "cd gods && npm test",
    "example": "node examples/basic-usage.js",
    "gods": "node claude-pantheon.js",
    "gods:help": "node claude-pantheon.js '/gods help'",
    "gods:init": "node claude-pantheon.js '/gods init'",
    "gods:status": "node claude-pantheon.js '/gods status'",
    "gods:projects": "node claude-pantheon.js '/gods projects'",
    "demo": "node examples/conversational-interface-demo.js",
    "quickstart": "node claude-pantheon.js '/gods help' && echo '\nRun: npm run gods:init'",
    "postinstall": "echo '\n⚠️  IMPORTANT: Run \"node install-claude-flow.js\" to install Claude-Flow (required)\n'",
    "install:claude-flow": "node install-claude-flow.js"
  },
  "keywords": [
    "ai",
    "agents",
    "orchestration",
    "claude-flow",
    "multi-agent",
    "workflow"
  ],
  "author": "Pantheon Team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bacoco/pantheon.git"
  }
}